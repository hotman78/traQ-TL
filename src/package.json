{
  "name": "traq-tl",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "dotenv -- bash -c 'SERVER_PATH=$SERVER_ADRESS next dev --turbopack'",
    "build": "mkdir -p ./.next/cache/images && dotenv -- bash -c 'SERVER_PATH=$SERVER_ADRESS NODE_ENV=production next build'",
    "start": "dotenv -- bash -c 'SERVER_PATH=$SERVER_ADRESS next start'",
    "dev-local": "SERVER_PATH='https://host.docker.internal:3000' next dev --turbopack",
    "lint": "next lint",
    "format": "prettier --write ./**/*.{ts,tsx}"
  },
  "dependencies": {
    "axios": "^1.8.3",
    "bootstrap": "^5.3.3",
    "dotenv-cli": "^8.0.0",
    "next": "15.2.2",
    "pako": "^2.1.0",
    "react": "^19.0.0",
    "react-bootstrap": "^2.10.9",
    "react-dom": "^19.0.0",
    "rehype-katex": "^7.0.1",
    "rehype-stringify": "^10.0.1",
    "remark": "^15.0.1",
    "remark-breaks": "^4.0.0",
    "remark-gfm": "^4.0.1",
    "remark-link-card": "^1.3.1",
    "remark-math": "^6.0.0",
    "remark-rehype": "^11.1.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/pako": "^2.0.3",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.2.2",
    "prettier": "^3.5.3",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
